{
    "name": "rangerx/laravel-notify",
    "description": "Laravel 5 Flash Notifications",
    "keywords": [
        "laravel",
        "flash",
        "notifications",
        "notify",
        "bootstrap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "RangerX",
            "email": "rangerx.1985@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.0.0"
    },
    "autoload": {
        "psr-4": {
            "RangerX\\Notify\\": "src/"
        },
        "files": [
            "src/helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "RangerX\\Notify\\NotifyServiceProvider"
            ],
            "aliases": {
                "Alert": "RangerX\\Notify\\Facades\\Notify"
            }
        }
    }
}
